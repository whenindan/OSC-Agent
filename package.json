{
  "name": "osc-agent",
  "version": "0.1.0",
  "description": "Open Source Contributing Agent - Autonomous CLI that acts as an AI open-source contributor",
  "main": "dist/index.js",
  "bin": {
    "osc": "dist/index.js"
  },
  "scripts": {
    "exec": "npx ts-node src/index.ts",
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --ext .ts",
    "typecheck": "tsc --noEmit",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\" \"tests/**/*.ts\"",
    "clean": "rimraf dist",
    "prepublishOnly": "npm run clean && npm run build",
    "prepare": "husky"
  },
  "keywords": [
    "ai",
    "automation",
    "github",
    "open-source",
    "cli",
    "agent",
    "contribution"
  ],
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "@types/dotenv": "^6.1.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.19.31",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "lint-staged": "^16.2.7",
    "prettier": "^3.2.5",
    "rimraf": "^6.1.2",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "axios": "^1.13.4",
    "dotenv": "^16.4.5",
    "type-fest": "^5.4.3",
    "yaml": "^2.4.1",
    "zod": "^3.22.4"
  }
}
